<!DOCTYPE html>
<html lang="en" ng-app="bestmallApp">
<head>
<%- include('header.ejs') %>
</head>

<body ng-controller="speechCon">
    <div class="fixedcloud"></div>
    <%- include('nav.ejs', {section: 'detail'}) %>    

        <div class="section-bg parallax" data-velocity=".6">
            <img src="blurvisual.png" alt="" srcset="">   
        </div>    
        <div class="visual-container cloud parallax" data-velocity=".2">
            <img src="cloud1.png" alt="" srcset="" style="right: 100px;">
            <img src="cloud2.png" alt="" srcset="" style="left: 100px;">
            <img src="cloud3.png" alt="" srcset="" style="right: 100px;">
        </div>    
        <div class="container-fuild  d-xl-none mobile-visual">
            <img class="img-fluid" src="mobile_history_main.png" alt="" srcset="">  
        </div>
        <div class="section-visual-container  d-none d-xl-block">
            <div class="visual">
               <img id="historymain" src="history_main.png" alt="" srcset="">
               <img id="film" class="animated infinite swing" src="film.png" alt="" srcset="">
               <img id="photo1" class="animated rollIn" src="photo1.png" alt="" srcset="">
               <img id="photo2" class="animated rollIn delay-1s" src="photo2.png" alt="" srcset="">
               <img id="photo3" class="animated rollIn delay-2s" src="photo3.png" alt="" srcset="">
               <img id="photo4" class="animated rollIn delay-3s" src="photo4.png" alt="" srcset="">
               <img id="heart1" class="animated infinite tada" src="heart1.png" alt="" srcset="">
               <img id="heart2" class="animated infinite tada delay-1s" src="heart1.png" alt="" srcset="">
               <img id="heart3" class="animated infinite tada" src="heart1.png" alt="" srcset="">
               <img id="heart4" class="animated infinite tada delay-2s" src="heart2.png" alt="" srcset="">
            </div> 
        </div>
        <div class="section-content-container" data-velocity=".9" >
            <div class="polaroid-container">
           
                <div class="polaroid" id="polaroid2017">
                    <a href="/history/2017"><img class="img-fluid" src="polaroid-01.jpg"></a>
                </div>
                <div class="polaroid" id="polaroid2016">
                     <a href="/history/2016"><img class="img-fluid" src="polaroid-02.jpg"></a>
                </div>
                <div class="polaroid" id="polaroid2015">
                    <a href="/history/2015"><img class="img-fluid" src="polaroid-03.jpg"></a>
                </div>
                <div class="polaroid" id="polaroid2014">
                    <a href="/history/2014"><img class="img-fluid"  src="polaroid-04.jpg"></a>
                </div>
                <div class="clearfix"></div>
            </div> 
        </div>    

    <script>
        $('.parallax').scrolly();
    </script>
    <%- include('footer.ejs') %>
    <%- include('./animation/history.ejs') %>
</body>

</html>
